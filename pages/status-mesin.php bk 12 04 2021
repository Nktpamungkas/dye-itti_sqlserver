	<div class="box box-primary">
      <div class="box-header">
        <h3 class="box-title">Celup Progress</h3>
      </div>
      <div class="box-body table-responsive">
        <table class="table table-bordered table-hover table-striped" style="width:100%" id="example2">
          <thead>
            <tr>
              <th width="111" style="width: 10px"><div align="center">NoMC</div></th>
              <th width="89" style="width: 50px"><div align="center">Urutan</div></th>
              <th width="202" style="width: 10px"><div align="center">Langganan</div></th>
              <th width="346" style="width: 150px"><div align="center">No Order</div></th>
              <th width="346" style="width: 150px"><div align="center">Kain</div></th>
              <th width="116" style="width: 10px"><div align="center">Warna</div></th>
              <th width="116" style="width: 10px"><div align="center">Lot</div></th>
              <th width="116" style="width: 10px"><div align="center">Target (Jam)</div></th>
              <th width="232" style="width: 100px"><div align="center">Progress</div></th>
              <th width="72" style="width: 10px"><div align="center">%</div></th>
            </tr>
          </thead>
          <tbody>
            <?php
			  $prsn=
    $col=0;
    $no=0;
    $qry=mysql_query("SELECT
   	id,
	GROUP_CONCAT( lot SEPARATOR '/' ) AS lot,
	if(COUNT(lot)>1,'Gabung Kartu','') as ket_kartu,
	no_mesin,
	no_urut,
	buyer,
	langganan,
	GROUP_CONCAT( no_order SEPARATOR ', ' ) AS no_order,
	no_resep,
	nokk,
	jenis_kain,
	warna,
	no_warna,
	sum(rol) as rol,
	sum(bruto) as bruto,
	proses,
	ket_status,
	target,
	tgl_delivery
FROM
	tbl_schedule 
WHERE
	NOT STATUS = 'selesai' 
GROUP BY
	no_mesin,
	no_urut 
ORDER BY
	id ASC");
  while ($rowd=mysql_fetch_array($qry)) {
	  $qPro=mysql_query("SELECT TIMESTAMPDIFF(MINUTE, b.tgl_buat, now())-if(ISNULL(TIMESTAMPDIFF(MINUTE, b.tgl_stop, b.tgl_mulai )),0,0) as lama FROM tbl_schedule a
LEFT JOIN tbl_montemp b ON a.id=b.id_schedule
WHERE a.no_mesin='$rowd[no_mesin]' AND b.status='sedang jalan' AND a.no_urut='$rowd[no_urut]' ORDER BY a.no_urut ASC");
			$dPro=mysql_fetch_array($qPro);
	  		if($dPro[lama]>0 and $rowd[target]!="" and $rowd[target]>"0"){
	  		$prsn=round(($dPro[lama]/($rowd[target]*60))*100);}else{ $prsn="0";}
       ?>
            <tr>
              <td align="center">
                <?php echo $rowd[no_mesin]; ?>
              </td>
              <td align="center">
                <?php echo $rowd[no_urut]; ?>
              </td>
              <td>
                <?php echo $rowd[langganan]."/".$rowd[buyer]; ?>
              </td>
              <td align="left"><?php echo $rowd[no_order]; ?></td>
              <td>
                <?php echo $rowd[jenis_kain]; ?>
              </td>
              <td align="center"><?php echo $rowd[warna]; ?></td>
              <td align="center"><?php echo $rowd[lot]; ?></td>
              <td align="center">
                <?php echo $rowd[target]; ?>
              </td>
              <td>
				<span class="label bg-abu"><?php echo $rowd[nokk]; ?></span>  
                <div class="progress progress-sm">						
                  <div class="progress-bar <?php if ($prsn=="100") { echo"progress-bar-success"; } elseif ($prsn>70) {
                    echo"progress-bar-primary";
                    } elseif ($prsn>50) {
                    echo"progress-bar-info";
                    } elseif ($prsn>30) {
                    echo"progress-bar-warning";
                    } else {
                    echo"progress-bar-danger";
                    } ?>" style="width:
                    <?php if ($prsn>1) {
          echo $prsn;
      } else {
          echo"1";
      } ?>%"></div>
                </div>
              </td>
              <td><span class="badge <?php if ($prsn==" 100") { echo"bg-green"; } elseif ($prsn>70) {
                  echo"bg-blue";
                  } elseif ($prsn>50) {
                  echo"bg-aqua";
                  } elseif ($prsn>30) {
                  echo"bg-yellow";
                  } else {
                  echo"bg-red";
                  } ?>">
                  <?php echo $prsn; ?>%</span></td>
            </tr>
            <?php
	  $no++;
  }?>
          </tbody>
        </table>
      </div>
      <!-- /.box-body -->
    </div>
		<div class="row">
	<?php 
	function tampil($mc,$shift){
			$qry=mysql_query("SELECT a.no_mesin,b.target,b.lama,b.jenis_kain,b.kategori_warna,b.proses,b.g_shift,b.operator FROM tbl_mesin a 
LEFT JOIN (
SELECT TIME_FORMAT(timediff(now(),b.tgl_buat),'%H:%i') as lama,a.no_mesin,a.jenis_kain,a.target,a.kategori_warna,a.proses,b.g_shift,b.operator FROM tbl_schedule a
LEFT JOIN tbl_montemp b ON a.id=b.id_schedule
WHERE b.status='sedang jalan' AND b.g_shift='$shift' ORDER BY a.no_urut ASC
) b ON a.no_mesin=b.no_mesin
WHERE a.no_mesin='$mc'");
			while ($row=mysql_fetch_array($qry)){
			$hasil[]=$row;
			}
			return $hasil;
		}		
	?>		
		<div class="col-xs-12">
          <div class="box box-solid">
			<div class="box-header with-border">
						<h3 class="box-title">Status Celup</h3>
						<div class="box-tools pull-right">
							<button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-minus"></i></button>
							<a href="pages/status-celup-full.php" class="btn btn-xs" data-toggle="tooltip" data-html="true" data-placement="bottom" title="FullScreen"><i class="fa fa-expand"></i></a>
						</div>
					</div>
            <div class="box-body table-responsive">
              <div id="carousel-example-generic" class="carousel slide" data-ride="carousel">                
                <div class="carousel-inner">
                <div class="item active">
		<table id="tblr1" class="table table-bordered table-hover table-striped" style="width:100%">
          <thead class="bg-blue">
            <tr>
			  <th><div align="center">No MC</div></th>
              <th><div align="center">Shift</div></th>
              <th><div align="center">Operator</div></th>
              <th><div align="center">Jenis Kain</div></th>
              <th><div align="center">Proses</div></th>
              <th><div align="center">Kategori Warna</div></th>
              <th><div align="center">Target</div></th>
